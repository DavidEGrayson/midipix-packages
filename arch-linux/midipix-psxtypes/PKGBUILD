# Maintainer: David Grayson <davidegrayson@gmail.com>

_target=x86_64-nt64-midipix

pkgname=$_target-psxtypes
conflicts=($_target-psxscl)
pkgver=r6.4d71ed8
pkgrel=1
pkgdesc=""
arch=('any')
url="http://git.midipix.org/$midipix_internal/psxtypes"
license=('unknown')
makedepends=()
options=(!strip)
source=("git://midipix.org/$midipix_internal/psxtypes")
sha256sums=('SKIP')

pkgver() {
  cd "${srcdir}/psxtypes"
  printf "r%s.%s" "$(git rev-list --count HEAD)" "$(git rev-parse --short HEAD)"
}

package() {
  cd "${srcdir}/psxtypes"
  make DESTDIR="${pkgdir}/usr/${_target}" install
}
